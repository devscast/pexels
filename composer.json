{
    "name": "devscast/pexels",
    "description": "PHP client for https://pexels.com",
    "keywords": [
        "pexels",
        "stock image",
        "images",
        "api"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Devscast\\Pexels\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devscast\\Pexels\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "bernard-ng",
            "email": "bernard@devscast.tech"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "squizlabs/php_codesniffer": "^3.7",
        "symplify/easy-coding-standard": "^11.0",
        "symfony/var-dumper": "^6.1|^7.0",
        "rector/rector": "^1.2",
        "phpunit/phpunit": "^10|^11.5"
    },
    "require": {
        "php": ">=8.2",
        "symfony/http-client": "^6.1|^7.0",
        "symfony/serializer": "^6.1|^7.0",
        "symfony/property-access": "^6.1|^7.0",
        "webmozart/assert": "^1.11"
    }
}
